			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="mat_no" class="java.lang.Integer">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="total_grade_points" class="java.math.BigDecimal"/>
		<field name="total_credit_hours" class="java.math.BigDecimal"/>
	</subDataset>
