	<queryString language="SQL">
		<![CDATA[Select concat(s.othername,' ',s.surname)AS student_name, 
	s.othername,
    concat(s.surname,',') AS sname,
    d.school_name,
    d.dean_email,
    s.academic_year,
    s.mat_no,
    s.major,
    s.year,
    s.program,
    c.course_code, 
    c.course_name, 
    c.credit_hours, 
    g.grade,
    p.SGPA
FROM grades g
JOIN student_registration_tbl s ON g.student_id = s.mat_no
JOIN vw_sgpa p ON s.mat_no = p.mat_no
JOIN schools_tbl d on s.school = d.school_id
JOIN courses c ON g.course_id = c.course_id


GROUP BY student_name, s.academic_year, s.othername, sname, s.mat_no, s.major, s.year, s.program, c.course_code, c.course_name, c.credit_hours, g.grade;]]>
